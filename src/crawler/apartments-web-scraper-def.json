{
    "family": "apartments-web-scraper",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "4096",
    "executionRoleArn": "arn:aws:iam::493504727387:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::493504727387:role/apartments-scraper-task-role",
    "containerDefinitions": [
      {
        "name": "scraper-container",
        "image": "493504727387.dkr.ecr.us-west-1.amazonaws.com/apartments-web-scraper:latest",
        "essential": true,
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/apartments-web-scraper",
            "awslogs-region": "us-west-1",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ]
  }
  